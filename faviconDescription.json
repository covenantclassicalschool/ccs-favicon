{
	"masterPicture": "assets/faviconMaster.png",
	"iconsPath": "/",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#e2cbb3",
			"margin": "4%",
			"assets": {
				"ios6AndPriorIcons": false,
				"ios7AndLaterIcons": false,
				"precomposedIcons": false,
				"declareOnlyDefaultIcon": true
			},
			"appName": "Covenant"
		},
		"desktopBrowser": {},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#e2cbb3",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": false,
					"medium": true,
					"big": false,
					"rectangle": false
				}
			},
			"appName": "Covenant"
		},
		"androidChrome": {
			"pictureAspect": "shadow",
			"themeColor": "#e2cbb3",
			"manifest": {
				"name": "Covenant",
				"startUrl": "https://www.covenantclassicalschool.org",
				"display": "standalone",
				"orientation": "portrait",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "blackAndWhite",
			"threshold": 50,
			"themeColor": "#785026"
		}
	},
	"settings": {
		"compression": 5,
		"scalingAlgorithm": "Spline",
		"errorOnImageTooSmall": false
	}
}